void drawPicture(float degree){
  // degree = 1 => 360°
  int index = floor(degree * EXTRACTION_SIZE * 2);
  
  // 360° = 1 * EXTRACTION_SIZE * 2 = EXTRACTION_SIZE * 2 => 0
  while(index >= EXTRACTION_SIZE) index -= EXTRACTION_SIZE;

  // 0 <= degree < 180
  if(degree > 0.5) for(int i = 0; i < NUM_LEDS; i++) leds[i] = getColor(index, i);

  // 180 <= degree < 360
  else for(int i = 0; i < NUM_LEDS; i++) leds[i] = getColor(index, NUM_LEDS - 1 - i);
  
  FastLED.show();
}


int turnCount = 0;
void oneTurn(){
  turn_start = (float) millis();
  last_rotation = current_turn;
  current_turn = 0;
  
  turnCount++;
  if(turnCount >= 2){
    turnCount = 0;
    IMAGE++;
    if(IMAGE >= N_IMAGE) IMAGE = 0;
  }
}

void setup() {
  FastLED.addLeds<APA102, DATA_PIN, CLOCK_PIN, BGR>(leds, NUM_LEDS);
  FastLED.setBrightness(3);
  attachInterrupt(digitalPinToInterrupt(INTERRUPT_PIN), oneTurn, RISING);
  LED_test();
}

float computeDegree(){
  return current_turn / last_rotation;
}

void loop() {
  // Update timer
  current_turn = (float) millis() - turn_start;
  if(last_rotation != 0) drawPicture(computeDegree());
}
